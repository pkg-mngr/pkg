    "linux-x64": "https://github.com/typst/typst/releases/download/v{{ version }}/typst-x86_64-unknown-linux-musl.tar.xz",
    "macos-arm64": "https://github.com/typst/typst/releases/download/v{{ version }}/typst-aarch64-apple-darwin.tar.xz",
