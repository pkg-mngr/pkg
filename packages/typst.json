      ],
      "macos-x64": [
        "tar -xf typst-x86_64-apple-darwin.tar.xz",
        "install typst-x86_64-apple-darwin/typst {{ pkg.bin_dir }}"
      ]
    },
    "latest": [
      "gh release list -R typst/typst --json tagName --jq '.[0].tagName' | cut -c 2-"
    ]
  }
