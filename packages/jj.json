{
  "$schema": "../package.schema.json",
  "name": "jj",
  "description": "A Git-compatible VCS that is both simple and powerful",
  "homepage": "https://jj-vcs.github.io/jj/",
  "version": "0.34.0",
  "sha256": {
    "linux-arm64": "39b5d689bcdc37256fe5f9ed9caccf5e73b310d1f86848fdf610a7bea9d75c14",
    "linux-x64": "1e8e1ede656fecba91ceb8248a76402a110017ad677d0ecfd1f2040f5543cfa3",
    "macos-arm64": "8893d34ff0c76e16fdf46ba8771ea2498294957aec5efb0618de15cc493aee27",
    "macos-x64": "807417125e574432b71c4568a0e2d6168c30af3b59e64ce02a45a54cf0880bd7"
  },
  "url": {
    "linux-arm64": "https://github.com/jj-vcs/jj/releases/download/v{{ version }}/jj-v{{ version }}-aarch64-unknown-linux-musl.tar.gz",
    "linux-x64": "https://github.com/jj-vcs/jj/releases/download/v{{ version }}/jj-v{{ version }}-x86_64-unknown-linux-musl.tar.gz",
    "macos-arm64": "https://github.com/jj-vcs/jj/releases/download/v{{ version }}/jj-v{{ version }}-aarch64-apple-darwin.tar.gz",
    "macos-x64": "https://github.com/jj-vcs/jj/releases/download/v{{ version }}/jj-v{{ version }}-x86_64-apple-darwin.tar.gz"
  },
  "scripts": {
    "install": {
      "linux-arm64": [
        "tar -xf jj-v{{ version }}-aarch64-unknown-linux-musl.tar.gz",
        "install jj {{ pkg.bin_dir }}"
      ],
      "linux-x64": [
        "tar -xf jj-v{{ version }}-x86_64-unknown-linux-musl.tar.gz",
        "install jj {{ pkg.bin_dir }}"
      ],
      "macos-arm64": [
        "tar -xf jj-v{{ version }}-aarch64-apple-darwin.tar.gz",
        "install jj {{ pkg.bin_dir }}"
      ],
      "macos-x64": [
        "tar -xf jj-v{{ version }}-x86_64-apple-darwin.tar.gz",
        "install jj {{ pkg.bin_dir }}"
      ]
    },
    "latest": [
      "gh release list -R jj-vcs/jj --json tagName --jq '.[0].tagName' | cut -c 2-"
    ],
    "completions": {
      "linux-arm64": [
        "jj util completion zsh > {{ pkg.completions.zsh }}/_jj"
      ],
      "linux-x64": [
        "jj util completion zsh > {{ pkg.completions.zsh }}/_jj"
      ],
      "macos-arm64": [
        "jj util completion zsh > {{ pkg.completions.zsh }}/_jj"
      ],
      "macos-x64": [
        "jj util completion zsh > {{ pkg.completions.zsh }}/_jj"
      ]
    }
  }
}
